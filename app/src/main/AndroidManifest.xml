            android:exported="false" />
        <activity
            android:name=".Activity.ShowResidencyDataActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.AddResidencyActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.ProfileActivity"
            android:exported="false"
